<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="#30D059" />
            <size
                android:width="60dp"
                android:height="60dp" />
        </shape>
    </item>
    <item
        android:bottom="14dp"
        android:left="14dp"
        android:right="14dp"
        android:top="14dp">
        <rotate
            android:fromDegrees="0"
            android:toDegrees="360"
            android:pivotX="50.0%"
            android:pivotY="50.0%">
            <bitmap android:src="@drawable/loading" />
        </rotate>
    </item>
</layer-list>